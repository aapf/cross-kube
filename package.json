{
  "name": "cross-kube",
  "version": "0.1.0",
  "description": "Kubernetes API Client Library for JavaScript",
  "keywords": [
    "kubernetes",
    "api",
    "frontend",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aapf/cross-kube.git"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "browser": {
    "./lib/runtime.js": "./lib/browser/runtime.js"
  },
  "types": "./lib/index.all.d.ts",
  "bin": {
    "cross-kube": "./bin/cross-kube.js"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cross-kube",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "bin",
    "lib",
    "src",
    "custom/docker"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "scripty",
    "codegen": "scripty",
    "build": "scripty",
    "build:lib": "scripty",
    "build:command": "scripty",
    "test": "scripty",
    "lint": "scripty",
    "precommit": "scripty",
    "local-link": "scripty",
    "local-unlink": "scripty"
  },
  "scripty": {
    "silent": true
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "abort-controller": "^3.0.0",
    "fetch-readablestream": "^0.2.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.1",
    "query-string": "^6.12.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.11.2",
    "@oclif/dev-cli": "^1.22.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@types/js-yaml": "^3.12.4",
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.7",
    "@types/rimraf": "^3.0.0",
    "debug": "^4.1.1",
    "deepmerge": "^4.2.2",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-config": "^26.0.1",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "rollup": "^2.10.4",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "scripty": "^2.0.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.2",
    "typescript-tslint-plugin": "^0.5.5"
  }
}
