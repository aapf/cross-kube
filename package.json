{
  "name": "cross-kube",
  "version": "0.2.0",
  "description": "Kubernetes API Client Library for JavaScript",
  "keywords": [
    "kubernetes",
    "api",
    "frontend",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aapf/cross-kube.git"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "browser": {
    "./lib/runtime.js": "./lib/browser/runtime.js"
  },
  "types": "./lib/index.all.d.ts",
  "bin": {
    "cross-kube": "./bin/cross-kube.js"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cross-kube",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "bin",
    "lib",
    "src",
    "custom/docker"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "clean": "scripty",
    "codegen": "scripty",
    "build": "scripty",
    "build:lib": "scripty",
    "build:command": "scripty",
    "test": "scripty",
    "lint": "scripty",
    "precommit": "scripty",
    "local-link": "scripty",
    "local-unlink": "scripty",
    "prepare": "husky install"
  },
  "scripty": {
    "silent": true
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "abort-controller": "^3.0.0",
    "fetch-readablestream": "^0.2.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.17.0",
    "@oclif/dev-cli": "^1.26.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "^4.0.0",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.36",
    "@types/node-fetch": "^2.5.8",
    "@types/rimraf": "^3.0.0",
    "debug": "^4.3.1",
    "deepmerge": "^4.2.2",
    "https-proxy-agent": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rollup": "^2.42.4",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "scripty": "^2.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.2.3",
    "typescript-tslint-plugin": "^1.0.1"
  }
}
